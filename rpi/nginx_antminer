server {
  listen 80;
  listen [::]:80;
  server_name cynitec.uk www.cynitec.uk;

  error_log /var/log/nginx/cynitec.uk.log;

  root /www/data;

  location / {
    index index.html;
  }

  location /antminer {
    proxy_pass http://192.168.0.4:80/cgi-bin/minerStatus.cgi;
    proxy_set_header Host $host;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "upgrade";
  }
}